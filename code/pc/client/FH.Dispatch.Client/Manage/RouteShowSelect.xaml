<Window x:Class="FH.Dispatch.Client.Manage.RouteShowSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="选择层位" Height="200" Width="400" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="110,58,0,0" Name="cb_racklayer" VerticalAlignment="Top" Width="120" />
        <Label Content="层位：" Height="28" HorizontalAlignment="Left" Margin="66,58,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="层" Height="28" HorizontalAlignment="Left" Margin="236,58,0,0" Name="label2" VerticalAlignment="Top" />
        <Label Content="仓位：" Height="28" HorizontalAlignment="Left" Margin="66,24,0,0" Name="label3" VerticalAlignment="Top" />
        <Label Content="lb_layername" Height="28" HorizontalAlignment="Left" Margin="110,24,0,0" Name="lb_layername" VerticalAlignment="Top" />
        <Button Content="确 定" Height="23" HorizontalAlignment="Left" Margin="110,104,0,0" Name="button1" VerticalAlignment="Top" Width="120" Style="{StaticResource css_btn}" Click="button1_Click" />
    </Grid>
</Window>
