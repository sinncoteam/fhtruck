<Window x:Class="FH.Dispatch.Client.Manage.TruckEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="叉车编辑" Height="200" Width="600" Style="{StaticResource css_window}" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <Label Content="叉车编号：" Height="28" HorizontalAlignment="Left" Margin="60,30,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="136,30,0,0" Name="tb_code" VerticalAlignment="Top" Width="317" />
        <Label Content="叉车名称：" Height="28" HorizontalAlignment="Left" Margin="60,64,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="136,64,0,0" Name="tb_name" VerticalAlignment="Top" Width="317" />
        <Button Content="保  存" Height="23" Style="{StaticResource css_btn}" HorizontalAlignment="Left" Margin="136,109,0,0" Name="btn_save" VerticalAlignment="Top" Width="146" Click="btn_save_Click" />
    </Grid>
</Window>
