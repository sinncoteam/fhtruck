<Application x:Class="FH.Dispatch.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Start.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary  Source="/Resource/Css/DataGridCss.xaml"/>
                <ResourceDictionary  Source="/Resource/Css/ButtonCss.xaml"/>
                <ResourceDictionary  Source="/Resource/Css/ComBoxCss.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="css_window" TargetType="{x:Type Window}">
                <Setter Property="Background" Value="#FFF8F8F8" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
